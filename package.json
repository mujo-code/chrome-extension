    "@svgr/webpack": "4.3.2",
    "@testing-library/react-hooks": "^1.1.0",
    "@tweenjs/tween.js": "^17.4.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "^24.9.0",
    "babel-plugin-named-asset-import": "^0.3.3",
    "babel-preset-react-app": "^9.0.1",
    "camelcase": "^5.2.0",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "cra-append-sw": "^2.6.1",
    "css-loader": "3.2.0",
    "date-fns": "^2.2.1",
    "dotenv": "8.1.0",
    "dotenv-expand": "5.1.0",
    "error-stack-parser": "^2.0.3",
    "eslint": "^5.16.0",
    "eventemitter3": "^4.0.0",
    "fake-indexeddb": "^2.1.1",
    "file-loader": "4.2.0",
    "filesize": "^4.2.0",
    "fs-extra": "8.1.0",
    "html-webpack-plugin": "4.0.0-beta.8",
    "i18next": "^17.0.15",
    "idb": "^4.0.4",
    "identity-obj-proxy": "3.0.0",
    "is-wsl": "^2.1.0",
    "jest": "^24.9.0",
    "jest-environment-jsdom-fourteen": "0.1.0",
    "jest-resolve": "24.9.0",
    "jest-watch-typeahead": "^0.4.0",
    "ky": "^0.14.0",
    "ky-universal": "^0.3.0",
    "mini-css-extract-plugin": "^0.8.0",
    "mkdirp": "^0.5.1",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "pnp-webpack-plugin": "1.5.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "prettier": "^1.18.2",
    "punycode": "^2.1.1",
    "puppeteer": "^1.20.0",
    "react": "^16.9.0",
    "react-app-polyfill": "^1.0.2",
    "react-dev-utils": "^9.0.3",
    "react-dom": "^16.9.0",
    "react-feather": "^2.0.3",
    "react-i18next": "^10.12.5",
    "react-modal": "^3.10.1",
    "react-test-renderer": "^16.9.0",
    "react-use-file-reader": "^1.0.0",
    "resolve": "1.12.0",
    "rimraf": "^3.0.0",
    "semver": "6.3.0",
    "sourcemapped-stacktrace": "^1.1.11",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "2.0.1",
    "universal-analytics": "^0.4.20",
    "url-loader": "^2.1.0",
    "use-color-scheme": "^1.1.1",
    "uuid": "^3.3.3",
    "webpack": "^4.40.2",
    "webpack-dev-server": "^3.8.1",
    "webpack-manifest-plugin": "2.0.4"
  },
  "scripts": {
    "start": "node scripts/watch.js",
    "start:dev": "DEVTOOLS=true node scripts/watch.js",
    "serve": "serve",
    "build:dev": "NODE_ENV=development node scripts/build.js && npm run build:post",
    "build:post": "npm run append:sw && npm run append:sm && npm run append:manifest",
    "prepend:plugins": "NODE_ENV=development node scripts/plugins-resolver",
    "build": "npm run prepend:plugins && PUBLIC_URL=./ node scripts/build.js && npm run build:post",
    "append:sw": "cra-append-sw --mode replace --skip-compile ./public/service-worker.js",
    "append:sm": "node scripts/modify-source-maps.js",
    "append:manifest": "node scripts/create-manifest.js",
    "test": "npm run build && node scripts/test.js --env=jsdom",
    "fmt": "prettier '{src,public}/**/*.js' --write",
    "lint": "eslint '{src,public}/**/*.js'",
    "dist": "npm run build && ./scripts/dist.sh",
    "dist:beta": "npm run build:beta && ./scripts/dist.sh",
    "append:beta": "node scripts/beta-decorate.js",
    "storybook": "start-storybook",
    "devtools": "react-devtools"
  },
  "browserslist": [
    "last 3 Chrome versions"
  ],
  "devDependencies": {
    "babel-loader": "^8.0.6",
    "change-case": "^3.1.0",
    "cosmiconfig": "^5.2.1",
    "gaze": "^1.1.3",
    "react-devtools": "^3.6.3",
    "serve": "^11.1.0"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "statments": 75,
        "branches": 60,
        "functions": 70,
        "lines": 75
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/background/storage/migrations/*.js"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
